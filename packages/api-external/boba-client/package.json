{
  "name": "@bobaboard/boba-client",
  "version": "0.0.1",
  "description": "Generated client for the BobaBoard API (with type safety!)",
  "type": "module",
  "files": [
    "./dist/**",
    "package.json"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client.d.ts",
        "default": "./dist/client.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "validate": " npx publint --pack npm"
  },
  "dependencies": {
    "@bobaboard/boba-api": "file:../boba-api"
  },
  "peerDependencies": {
    "@bobaboard/boba-api": "*"
  },
  "author": "",
  "license": "MIT"
}
